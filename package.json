{
  "name": "midifile-performer",
  "version": "0.4.3",
  "description": "A library allowing for real-time expressive performance of MIDI files",
  "main": "dist/node/MidifilePerformer.js",
  "browser": "dist/browser/MidifilePerformer.js",
  "engines": {
    "node": ">=18.15.0"
  },
  "directories": {
    "test": "test"
  },
  "np": {
    "yarn": false,
    "contents": "dist/"
