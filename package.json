  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/scrime-u-bordeaux/midifile-performer-js/issues"
  },
  "homepage": "https://github.com/scrime-u-bordeaux/midifile-performer-js#readme",
  "devDependencies": {
    "np": "^8.0.4",
    "tap-spec": "^2.2.2",
    "tape": "^5.5.3"
